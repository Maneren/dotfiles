<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xkbConfigRegistry SYSTEM "xkb.dtd">
<xkbConfigRegistry version="1.1">
  <layoutList>
    <layout>
      <configItem>
        <name>cz</name>
        <!-- Keyboard indicator for Czech layouts -->
        <shortDescription>cs</shortDescription>
        <description>Czech</description>
        <countryList>
          <iso3166Id>CZ</iso3166Id>
        </countryList>
        <languageList>
          <iso639Id>ces</iso639Id>
        </languageList>
      </configItem>
      <variantList>
        <variant>
          <configItem>
            <name>sexy</name>
            <description>Czech (sexy, AltGr for diacritics)</description>
          </configItem>
        </variant>
      </variantList>
    </layout>
  </layoutList>
</xkbConfigRegistry>
